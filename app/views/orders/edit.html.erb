<h1>Editing order</h1>

<% form_for(@order) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label "Номер" %><br />
    <%= f.text_field :number %>
  </p>
  <p>
    <%= f.label "Дата" %><br />
    <%= f.text_field :date %>
  </p>
  <p>
    <%= f.label "Внутрений номер" %><br />
    <%= f.text_field :intnumber %>
  </p>
  <p>
    <%= f.label "Содержание" %><br />
    <%= f.text_area :desc %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @order %> |
<%= link_to 'Back', orders_path %>